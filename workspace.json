{
  "version": 2,
  "projects": {
    "avai": {
      "root": "apps/avai",
      "sourceRoot": "apps/avai/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/avai",
            "main": "apps/avai/src/main.ts",
            "tsConfig": "apps/avai/tsconfig.app.json",
            "assets": ["apps/avai/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/avai/src/environments/environment.ts",
                  "with": "apps/avai/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "avai:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/avai/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/avai"],
          "options": {
            "jestConfig": "apps/avai/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "components-account": {
      "root": "libs/components/account",
      "sourceRoot": "libs/components/account/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/components/account/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/components/account"],
          "options": {
            "jestConfig": "libs/components/account/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "components-balance": {
      "root": "libs/components/balance",
      "sourceRoot": "libs/components/balance/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/components/balance/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/components/balance"],
          "options": {
            "jestConfig": "libs/components/balance/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "hooks": {
      "root": "libs/hooks",
      "sourceRoot": "libs/hooks/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/hooks/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/hooks"],
          "options": {
            "jestConfig": "libs/hooks/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-contracts": {
      "root": "libs/shared/contracts",
      "sourceRoot": "libs/shared/contracts/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/contracts/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/contracts"],
          "options": {
            "jestConfig": "libs/shared/contracts/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "shared-styles": {
      "root": "libs/shared/styles",
      "sourceRoot": "libs/shared/styles/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/shared/styles/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/shared/styles"],
          "options": {
            "jestConfig": "libs/shared/styles/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "test-contract": {
      "root": "apps/test-contract",
      "sourceRoot": "apps/test-contract/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "outputPath": "dist/apps/test-contract",
            "main": "apps/test-contract/src/main.ts",
            "tsConfig": "apps/test-contract/tsconfig.app.json",
            "assets": ["apps/test-contract/src/assets"]
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "apps/test-contract/src/environments/environment.ts",
                  "with": "apps/test-contract/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "test-contract:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/test-contract/**/*.ts"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/test-contract"],
          "options": {
            "jestConfig": "apps/test-contract/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "ursa": {
      "root": "apps/ursa",
      "sourceRoot": "apps/ursa",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/next:build",
          "outputs": ["{options.outputPath}"],
          "options": {
            "root": "apps/ursa",
            "outputPath": "dist/apps/ursa"
          },
          "configurations": {
            "production": {}
          }
        },
        "serve": {
          "executor": "@nrwl/next:server",
          "options": {
            "buildTarget": "ursa:build",
            "dev": true
          },
          "configurations": {
            "production": {
              "buildTarget": "ursa:build:production",
              "dev": false
            }
          }
        },
        "export": {
          "executor": "@nrwl/next:export",
          "options": {
            "buildTarget": "ursa:build:production"
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/apps/ursa"],
          "options": {
            "jestConfig": "apps/ursa/jest.config.js",
            "passWithNoTests": true
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ursa/**/*.{ts,tsx,js,jsx}"]
          }
        }
      }
    },
    "ursa-e2e": {
      "root": "apps/ursa-e2e",
      "sourceRoot": "apps/ursa-e2e/src",
      "projectType": "application",
      "targets": {
        "e2e": {
          "executor": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ursa-e2e/cypress.json",
            "tsConfig": "apps/ursa-e2e/tsconfig.e2e.json",
            "devServerTarget": "ursa:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ursa:serve:production"
            }
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["apps/ursa-e2e/**/*.{js,ts}"]
          }
        }
      }
    },
    "util": {
      "root": "libs/util",
      "sourceRoot": "libs/util/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": ["libs/util/**/*.{ts,tsx,js,jsx}"]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": ["coverage/libs/util"],
          "options": {
            "jestConfig": "libs/util/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nrwl/next"
  },
  "generators": {
    "@nrwl/react": {
      "application": {
        "babel": true
      }
    },
    "@nrwl/next": {
      "application": {
        "style": "@emotion/styled",
        "linter": "eslint"
      }
    }
  },
  "defaultProject": "ursa"
}
